{
    "manifest_version": 2,
    "name": "Supreme Tools",
    "version": "2.0.1",
    "description": "Hand-bot",
    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },
    "browser_action": {
        "default_icon": "img/16.png",
        "default_popup": "hand/index.html",
        "default_title": "Supreme_Hand"
    },
    "content_scripts": [
        {
            "js": [
                "js/index.js"
            ],
            "matches": [
                "https://www.supremenewyork.com/*",
                "https://supremenewyork.com/*",
                "<all_urls>"
            ]
        }
    ]
}